<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.lidh.blog.Dao.websiteDao">
    <select id="get" resultType="website">
        select * from website;
    </select>
    <update id="set" parameterType="website">
        update  website set `name`=#{name},title=#{title},cover=#{cover}
    </update>
</mapper>